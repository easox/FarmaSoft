	
	
	
	//med_a y med_b
	 CMedProv medprov_a (23.55,med_a);
	 CMedProv medprov_b (2.55,med_b);
	 
	 list <CMedProv*> l_medprov_a;
	 l_medprov_a.push_back(medprov_a, medprov_b);
	 list <CMedProv*> l_medprov_b;
	 l_medprov_b.push_back( medprov_b );
	 
	CProveedor P1(1, "Cecofar",l_medprov_a);
	CProveedor P2(2 , "Cofares",l_medprov_b);

	
